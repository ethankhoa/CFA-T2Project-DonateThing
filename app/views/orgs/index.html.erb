<p id="notice"><%= notice %></p>

<h1>Orgs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Org type</th>
      <th>Phone</th>
      <th>Website</th>
      <th>Logo</th>
      <th>Images</th>
      <th>User</th>
      <th>Googleplaceid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orgs.each do |org| %>
      <tr>
        <td><%= org.name %></td>
        <td><%= org.org_type %></td>
        <td><%= org.phone %></td>
        <td><%= org.website %></td>
        <td><%= org.logo %></td>
        <td><%= org.images %></td>
        <td><%= org.user %></td>
        <td><%= org.googleplaceid %></td>
        <td><%= link_to 'Show', org %></td>
        <td><%= link_to 'Edit', edit_org_path(org) %></td>
        <td><%= link_to 'Destroy', org, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Org', new_org_path %>
